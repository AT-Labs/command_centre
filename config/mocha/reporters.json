{
    "reporterEnabled": "mocha-junit-reporter, mochawesome",
    "mochaJunitReporterReporterOptions": {
        "mochaFile": "./test-reports/unit-tests/junit/test-result.xml",
        "testCaseSwitchClassnameAndName": false
