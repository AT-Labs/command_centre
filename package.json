  "scripts": {
    "start": "node scripts/start.js",
    "start:e2e": "cross-env REACT_APP_DISABLE_ACTIVE_DIRECTORY_LOGIN=true node scripts/start.js",
    "build-prod": "cross-env NODE_ENV=production BABEL_ENV=production node scripts/build.js",
    "build-preprod": "cross-env NODE_ENV=preprod BABEL_ENV=production node scripts/build.js",
    "build-test": "cross-env NODE_ENV=test BABEL_ENV=production node scripts/build.js",
    "build-dev": "cross-env NODE_ENV=development BABEL_ENV=production node scripts/build.js",
    "eslint": "eslint src cypress/integration --ext=[.js,.jsx] --max-warnings=0",
    "stylelint": "stylelint \"**/*.scss\"",
    "test": "cross-env NODE_ENV=test mocha --opts ./config/mocha/mocha.opts --exit",
    "test:watch": "cross-env NODE_ENV=test mocha --watch --opts ./config/mocha/mocha.opts",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --opts ./config/mocha/mocha.opts --exit",
    "e2e": "start-server-and-test start:e2e http-get://localhost:3000 e2e:run",
    "e2e:run": "cypress run --browser electron",
    "e2e:spec": "cypress run --spec 'cypress/integration/vehicles.spec.js'",
    "e2e:browser": "cypress open --browser chrome",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      "@babel/polyfill",
      "react-app"
    ]
  },
  "sideEffects": [
    "leaflet.geometryutil.js"
  ],
  "nyc": {
    "reporter": [
      "lcovonly",
      "text"
    ],
    "extension": [
      ".jsx"
    ],
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/*.test.jsx"
    ],
    "report-dir": "./test-reports/unit-tests/coverage"
  }
