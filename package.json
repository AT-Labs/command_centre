  "scripts": {
    "start": "node scripts/start.js",
    "start:e2e": "cross-env NODE_ENV=test REACT_APP_DISABLE_ACTIVE_DIRECTORY_LOGIN=true node scripts/start.js",
    "build-prod": "cross-env NODE_ENV=production BABEL_ENV=production node scripts/build.js",
    "build-preprod": "cross-env NODE_ENV=preprod BABEL_ENV=production node scripts/build.js",
    "build-test": "cross-env NODE_ENV=test BABEL_ENV=production node scripts/build.js",
    "build-dev": "cross-env NODE_ENV=development BABEL_ENV=production node scripts/build.js",
    "build-local": "cross-env NODE_ENV=local BABEL_ENV=production node scripts/build.js",
    "eslint": "eslint src cypress/integration --ext=[.js,.jsx] --max-warnings=0",
    "stylelint": "stylelint \"**/*.scss\"",
    "test": "cross-env NODE_ENV=test jest --config ./config/jest/jest.config.js && mocha --config ./config/mocha/.mocharc.json --exit",
    "test:watch": "cross-env NODE_ENV=test mocha --config ./config/mocha/.mocharc.json --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc --report-dir ./test-reports/mocha mocha --config ./config/mocha/.mocharc.json --exit && jest --config ./config/jest/jest.config.js",
    "e2e": "start-server-and-test start:e2e http-get://localhost:3000 e2e:run",
    "e2e:run": "cypress run --browser electron",
    "e2e:node1": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node1'",
    "e2e:node2": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node2'",
    "e2e:node3": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node3'",
    "e2e:node4": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node4'",
    "e2e:node5": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node5'",
    "e2e:node6": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node6'",
    "e2e:node7": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node7'",
    "e2e:node8": "start-server-and-test start:e2e http-get://localhost:3000 'cypress run --browser electron --config integrationFolder=cypress/integration/node8'",
    "e2e:spec": "cypress run --spec 'cypress/integration/vehicles.spec.js'",
    "e2e:browser": "cypress open --browser chrome",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "jest": "jest --config ./config/jest/jest.config.js"
  },
  "babel": {
    "plugins": [
      "lodash",
      "@babel/transform-runtime"
    ],
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  },
  "sideEffects": [
    "leaflet.geometryutil.js"
  ],
  "nyc": {
    "reporter": [
      "lcovonly",
      "text"
    ],
    "extension": [
      ".jsx"
    ],
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/*.test.jsx"
    ],
    "report-dir": "./test-reports/unit-tests/coverage"
  }
